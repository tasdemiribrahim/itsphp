<phpunit bootstrap="./application/bootstrap.php" colors="true">	<testsuite name="WJGilmore">		<directory>./</directory>	</testsuite>	<filter>		<whitelist>			<directory suffix=".php">../application/</directory>			<exclude>				<file>../application/Bootstrap.php</file>				<file>../application/controllers/ErrorController.php</file>				<directory suffix=".phtml">../application/</directory>			</exclude>		</whitelist>	</filter>	<logging>		<log type="coverage-html" target="../../error" charset="UTF-8" yui="true" highlight="true" lowUpperBound="50" highLowerBound="80" />		<log type="testdox-html" target="../../error/testdox.html" />	</logging></phpunit>